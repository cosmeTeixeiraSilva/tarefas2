generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//Criando a Tabela tarefas
model tarefas {
  id        Int    @id @unique @default(autoincrement())
  uuid      String @unique @default(uuid())
  descricao String
  status    String @default("Pendente")
  cidade    String @default("Lavras")
}
